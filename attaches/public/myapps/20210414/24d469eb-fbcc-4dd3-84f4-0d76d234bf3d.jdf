        "ICON": null,
        "URI": "searchApps.uc",
        "BODY": "{\n  \"uuid\": \"7b91a950-15a8-4aba-a5c1-0bbfe82691f8\",\n  \"html\": \"<section class=\\\"section\\\">\\n  <div class=\\\"container-fluid\\\">\\n    <div class=\\\"row\\\">\\n      <div class=\\\"col-4\\\">\\n        <div class=\\\"form-group row\\\">\\n          <label class=\\\"col-sm-5 col-form-label\\\">Apps:</label>\\n          <div class=\\\"col-sm-7\\\">\\n            <input target=\\\"lstApps\\\" class=\\\"form-control\\\" type=\\\"search\\\" name=\\\"search\\\" >\\n          </div>\\n        </div>\\n        <ul jj-id=\\\"lstApps\\\" class=\\\"list-group jj-active\\\" j-for=\\\"row in data.apps\\\">\\n          <li jj-command=\\\"getAppInfo('${row.ID}')\\\"  class=\\\"list-group-item\\\">\\n            <i class=\\\"fa fa-database mr-1\\\" aria-hidden=\\\"true\\\"></i> ${row.NAME}</li>\\n        </ul>\\n      </div>\\n      <div class=\\\"col-4\\\">\\n        <div class=\\\"form-group row\\\">\\n          <label class=\\\"col-sm-5 col-form-label\\\">display:</label>\\n          <div class=\\\"col-sm-7\\\">\\n            <input target=\\\"lstDispColumn\\\" class=\\\"form-control\\\" type=\\\"search\\\" name=\\\"search\\\" >\\n          </div>\\n        </div>\\n        <ul jj-id=\\\"lstDispColumn\\\" class=\\\"list-group jj-active\\\" j-for=\\\"row in data.colums\\\" j-page=300>\\n          <li jj-command=\\\"setDisplyField('${row.COLUMN_NAME}')\\\" class=\\\"list-group-item ${row.key=='PK'? 'active':''}\\\">\\n            <i class=\\\"fa fa-code mr-1\\\" aria-hidden=\\\"true\\\"></i> ${row.COLUMN_NAME}/${row.COLUMN_DOMAIN}\\n            <span class=\\\"badge badge-info badge-pill\\\">${row.NULLABLE}</span>\\n          </li>\\n        </ul>\\n      </div>\\n    </div>\\n  </div>\\n</section>\\n\",\n  \"less\": \"\",\n  \"script\": \"form.config={\\n  \\\"data\\\":{\\n    \\\"workspace\\\":\\\"\\\",\\n    \\\"table\\\":\\\"\\\",\\n    \\\"keyField\\\": \\\"\\\",\\n    \\\"displayField\\\": \\\"\\\",\\n    \\\"apps\\\":[],\\n    \\\"colums\\\":[],\\n  }\\n};\\n\\nform.bo = new JJReact(form, form.config);\\n\\nJinja.driverApi.getTreeByFolderId(\\\"26741e50-61b4-4eee-9683-327faa46e3f1\\\")\\n.then(result=>{\\n  result.forEach(row=>{\\n    if(row.TYPE == \\\"formapp\\\"){\\n      form.config.data.apps.push({\\n        \\\"NAME\\\": row.NAME,\\n        \\\"ID\\\": row.ID\\n      })\\n    }\\n  })\\n})\\n\\nform.getAppInfo=(fileId)=>{\\n  form.config.data.workspace = \\\"\\\";\\n  form.config.data.table = \\\"\\\";\\n  form.config.data.keyField = \\\"\\\";\\n  form.config.data.colums = [];\\n  Jinja.driverApi.readFile(fileId).then(result=>{\\n    let appInfo = Jinja.global.json(result.BODY)\\n  \\tform.config.data.workspace = appInfo.datainfo.workspace;\\n \\t\\tform.config.data.table = appInfo.datainfo.table;\\n \\t\\tform.config.data.keyField = appInfo.datainfo.keyField;\\n  \\tform.config.data.colums = Jinja.global.json(result.BODY).columns;\\n  })\\n}\\n\\nform.setDisplyField=(name)=>form.config.data.displayField = name;\\n\\nform.modalOk = ()=>form.return({\\n  \\\"workspace\\\": form.config.data.workspace,\\n  \\\"table\\\": form.config.data.table,\\n  \\\"keyField\\\": form.config.data.keyField,\\n  \\\"displayField\\\": form.config.data.displayField\\n});  \\n\\n/**\\n * 필터를 이용하여 검색함\\n */\\nform.addEventListener(\\\"input\\\", e=>{\\n  let target = e.target.getAttribute('target');\\n  if(target){\\n    form[target].querySelectorAll('li').forEach(li=>{\\n        li.hidden = !li.innerText.includes(e.target.value);\\n    })\\n  }\\n  \\n})\"\n}",
