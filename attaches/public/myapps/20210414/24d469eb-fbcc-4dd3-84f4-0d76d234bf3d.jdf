   .catch(()=>{ alert(\\\"조회실패\\\")});\\n}\\n\\nform.setWorkspace=(workspace)=>{\\n  form.config.data.workspace = workspace;\\n  dao.setWorkspace(workspace);\\n  form.config.data.tables = [];\\n  form.config.data.colums = [];\\n  dao.getTables()\\n    .then(data=>form.config.data.tables = data.rows)\\n    .catch(()=>{});\\n}\\nform.setTable=(table)=>{\\n  form.config.data.table = table;\\n  dao.getColumns(table)\\n    .then(data=>{\\n       form.config.data.colums = data.rows;\\n    let pks = [];\\n    data.rows.forEach(row=>{\\n      if(row.key==\\\"PK\\\") pks.push(row.COLUMN_NAME);\\n    });\\n    form.config.data.keyField = pks.join(\\\",\\\");\\n  })\\n    .catch(()=>{});\\n}\\n\\nform.setKeyField=(column)=>{\\n  form.config.data.keyField = column;\\n}\\nform.setDisplayField=(column)=>{\\n  form.config.data.displayField = column;\\n}\\nform.formLoad=()=>{\\n  form.getConnection();\\n}\\n\\nform.modalOk = ()=>form.return({\\n  \\\"workspace\\\": form.config.data.workspace,\\n  \\\"table\\\": form.config.data.table,\\n  \\\"keyField\\\": form.config.data.keyField\\n});  \\n\\n/**\\n * 필터를 이용하여 검색함\\n */\\nform.addEventListener(\\\"input\\\", e=>{\\n  let target = e.target.getAttribute('target');\\n  if(target){\\n    form[target].querySelectorAll('li').forEach(li=>{\\n        li.hidden = !li.innerText.includes(e.target.value);\\n    })\\n  }\\n  \\n})\"\n}",
