        "URI": "pivotView.640.uc",
        "BODY": "{\r\n  \"uuid\": \"89e765b4-beba-4fd2-b1b3-19319da79ba8\",\r\n  \"html\": \"<button jj-command=\\\"pivot\\\" hidden>Pivot</button>\\n\\n<div jj-id=\\\"table\\\"></div>\\n\\n<div jj-id=\\\"pv\\\">\\n  <div class=\\\"table-head-warp\\\"></div>\\n  <div class=\\\"table-body-warp\\\"></div>\\n</div>\\n\\n<ul jj-id=\\\"piItems\\\" class=\\\"list-group mt-2\\\"></ul>\\n<ul jj-id=\\\"piRows\\\" class=\\\"list-group mt-2\\\"></ul>\\n<ul jj-id=\\\"piCols\\\" class=\\\"list-group mt-2\\\"></ul>\",\r\n  \"less\": \"[jj-id=\\\"pv\\\"]{ \\n  position: relative;\\n  width:100%;\\n  height:300px;\\n  //border: 1px solid blue;\\n  display: flex;\\n  flex-direction: column;\\n  //border-left: 1px solid #ccc;\\n\\n  .table-head-warp {\\n    display: inline-block;\\n    flex-grow: 0;\\n    overflow: hidden;\\n    overflow-y: scroll;\\n    flex: none;\\n    position: relative;\\n\\n    &::-webkit-scrollbar-button:start:decrement,\\n    &::-webkit-scrollbar-button:end:increment {\\n      height: 0px;\\n    }\\n    &::-webkit-scrollbar-track {\\n      background: #efefef;\\n      -webkit-border-radius: 0px !important;\\n      border-radius: 0px !important;\\n      -webkit-box-shadow: none;\\n    }\\n    &::-webkit-scrollbar-thumb {\\n      display: none;\\n    }\\n  }\\n  .table-body-warp {\\n    flex-grow: 1;\\n    overflow: auto;\\n    overflow-y: scroll;\\n    margin-top: -1px;\\n    margin-bottom: -5px;\\n    position: relative;\\n  }\\n\\n  table {\\n    table-layout: fixed;\\n    width: 1%;\\n    border: 1px solid var(--secondary);\\n    border-collapse: collapse;\\n    background-color: var(--white);\\n    col{\\n      //min-width:100px;\\n    }\\n    th, td {\\n      border: 1px solid var(--secondary);\\n      padding: 3px;\\n      font-size: 12px;\\n      color: black;    \\n      //width:100px;\\n      //min-width:100px;\\n    }\\n    th {\\n      text-align: center;\\n      background-color: var(--light);\\n    }\\n    td {\\n      text-align: right;\\n    }\\n\\n    tr.sub-total, tr.row-total{\\n      background-color: var(--light);\\n      td, th{\\n        //font-weight: 700!important;\\n      }\\n    }\\n  }\\n\\n  table.fix-table {\\n    position: absolute;\\n    left: 0px;\\n    top: 0px;\\n    background-color: whitesmoke;\\n  }\\n\\n}\\n\\n\\n[jj-id=\\\"piItems\\\"]{\\n  width:200px;\\n  float: left;\\n}\\n[jj-id=\\\"piRows\\\"]{\\n  width:200px;\\n  float: left;\\n}\\n[jj-id=\\\"piCols\\\"]{\\n  display: block;\\n  .list-group-item{\\n    width:150px;\\n    float: left;\\n    overflow: hidden;\\n    white-space: nowrap;\\n    text-overflow: ellipsis;\\n  }\\n}\\n\\n.list-group:empty:before{\\n  content:\\\"선택하세요\\\"\\n}\",\r\n  \"script\": \"/**\\n *    COM_CD \\n  ,IN_PROD_DIV \\n  ,MTT_CD \\n  ,OBJ_YYMMDD \\n  ,PLC_CD \\n  ,READ_VAL \\n  ,READ_VAL_00TM \\n  ,READ_VAL_01TM \\n  ,READ_VAL_02TM \\n  ,READ_VAL_03TM  \\n*/\\nlet pOption ={\\n  \\\"dataSet\\\": [],\\n  \\\"rows\\\": [\\\"YYYY\\\",\\\"MM\\\"],\\n  \\\"cols\\\": [ \\\"COM_CD\\\",\\\"MTT_CD\\\"],\\n  \\\"datas\\\": [\\n    {\\n      \\\"name\\\": \\\"count\\\",\\n      \\\"label\\\":\\\"건수\\\",\\n      \\\"field\\\": \\\"READ_VAL\\\",\\n      \\\"math\\\": \\\"count\\\"\\n    },\\n    {\\n      \\\"name\\\": \\\"sum\\\",\\n      \\\"label\\\":\\\"합계\\\",\\n      \\\"field\\\": \\\"READ_VAL\\\",\\n      \\\"math\\\": \\\"sum\\\"\\n    }\\n  ],\\n  \\\"numFormat\\\": \\\"INT\\\"\\n};\\n\\nform.formLoad=(p)=>{\\n  debugger;\\n  //p.gridEl.options.dataSource;\\n  form.data = p.gridEl.getData();\\n  \\n  pOption.rows =[\\\"REG_USER_NAME\\\"];\\n  pOption.cols =[\\\"EDT_USER_NAME\\\"];\\n  pOption.datas =[\\n    {\\n      \\\"name\\\": \\\"count\\\",\\n      \\\"label\\\":\\\"건수\\\",\\n      \\\"field\\\": p.gridEl.options.dataSource.keyField,\\n      \\\"math\\\": \\\"count\\\"\\n    } \\n  ];\\n  \\n  \\n  \\n  p.gridEl.options.colModel.forEach(col=>{\\n    if(pOption.rows.includes(col.field)){\\n      Jinja.dom.append(form.piRows, `<li class=\\\"list-group-item\\\" data-key='${col.field}'>${col.label}</li>`);\\n    }\\n    if(pOption.cols.includes(col.field)){\\n      Jinja.dom.append(form.piCols, `<li class=\\\"list-group-item\\\" data-key='${col.field}'>${col.label}</li>`);\\n    }\\n    Jinja.dom.append(form.piItems, `<li class=\\\"list-group-item\\\" data-key='${col.field}'>${col.label}</li>`);\\n  })\\n  setTimeout(e=>{\\n  \\tform.pivot();     \\n  }, 500)\\n  \\n  \\n}\\n\\n// Jinja.ajax.post(`/v61/jbatis/fems/excute/get/pivot.sql`)\\n//   .then((result)=>{\\n//   form.data = result.rows;\\n//   form.querySelector(`[jj-command=\\\"pivot\\\"]`).hidden = false;\\n  \\n//   Object.keys(form.data[0]).forEach(key=>{\\n//     if(pOption.rows.includes(key)){\\n//       Jinja.dom.append(form.piRows, `<li class=\\\"list-group-item\\\" data-key='${key}'>${key}</li>`);\\n//     }\\n//     if(pOption.cols.includes(key)){\\n//       Jinja.dom.append(form.piCols, `<li class=\\\"list-group-item\\\" data-key='${key}'>${key}</li>`);\\n//     }\\n//     Jinja.dom.append(form.piItems, `<li class=\\\"list-group-item\\\" data-key='${key}'>${key}</li>`);\\n//   })\\n//   form.pivot(); \\n// });\\n \\n\\nnew JJSortable([form.piItems, form.piRows, form.piCols]);\\n\\nform.addEventListener(\\\"JJSortableEnd\\\", e=>{\\n  if(e.detail.index==0){\\n\\t  pOption.rows =[];\\n  \\tform.piRows.querySelectorAll('[data-key]').forEach(el=>pOption.rows.push(el.dataset.key))\\n\\t  pOption.cols =[];\\n  \\tform.piCols.querySelectorAll('[data-key]').forEach(el=>pOption.cols.push(el.dataset.key))\\n    form.pivot(); \\n  }\\n})\\n\\n\\n\\n\\nform.pivot=()=>{\\n  pOption.dataSet = form.data;\\n  pOption.pvTable = jsonPivot(pOption);\\n\\n  sortRows(pOption);\\n\\n  \\n  form.pv.querySelector('.table-body-warp').innerHTML = json2html(pOption);\\n  \\n  meargeCol(pOption);\\n\\n/* : table wrap  \\n  let bodyTable = form.pv.querySelector('.table-body-warp table');\\n  let headTable = bodyTable.cloneNode(true);\\n  headTable.querySelector('tbody').remove();\\n  bodyTable.querySelector('thead').remove();\\n  form.pv.querySelector('.table-head-warp').$empty().appendChild(headTable);\\n*/  \\n\\n  subSum(pOption);\\n  meargeRow(pOption);\\n\\n  //totalSum\\n  //totalSum(pOption);\\n  //fixed(3);\\n  \\n}\\n\\nfunction fixed(fixColIndex) {\\n let headFixTable = form.pv.querySelector('.table-head-warp table').cloneNode(true);\\n    \\n  let bodyFixTable = form.pv.querySelector('.table-body-warp table').cloneNode(true);\\n\\n  headFixTable.classList.add('fix-table');\\n  bodyFixTable.classList.add('fix-table');\\n  \\n  form.pv.querySelector('.table-head-warp').appendChild(headFixTable);\\n  form.pv.querySelector('.table-body-warp').appendChild(bodyFixTable);\\n\\n  headFixTable\\n    .querySelectorAll(`colgroup >*:nth-child(${fixColIndex}) ~ *`)\\n    .forEach(el => el.remove());\\n\\n  headFixTable.querySelectorAll('thead tr').forEach(tr => {\\n    tr.querySelectorAll(\\n      `:scope >*:nth-child(${fixColIndex}) ~ *`\\n    ).forEach(el => el.remove());\\n  });\\n \\n  bodyFixTable\\n    .querySelectorAll(`colgroup >*:nth-child(${fixColIndex}) ~ *`)\\n    .forEach(el => el.remove());\\n\\n  bodyFixTable.querySelectorAll('tbody tr').forEach(tr => {\\n    tr.querySelectorAll(\\n      `:scope >*:nth-child(${fixColIndex}) ~ *`\\n    ).forEach(el => el.remove());\\n  });\\n\\n  bodyFixTable.querySelectorAll('tfoot tr').forEach(tr => {\\n    tr.querySelectorAll(\\n      `:scope >*:nth-child(${fixColIndex}) ~ *`\\n    ).forEach(el => el.remove());\\n  });\\n}\\n\\nfunction subSum(options){\\n  let cellIndex\\n  for(cellIndex=0; cellIndex<options.rows.length-1; cellIndex++){\\n    //options.rows.forEach((row, cellIndex)=>{  \\n    let sumTr; \\n    let subTopTr;\\n    let rowSumIndex=cellIndex;\\n    // 행을 이동하면서 부분합을 구한다  \\n    if(cellIndex < (options.rows.length-1)){\\n      form.pv.querySelectorAll('tbody tr').forEach((tr, trIndex)=>{\\n        // 합계행이 없으면 현재행을 선택한다\\n        // 첫번째 시도에 해당된다\\n        if(!sumTr){\\n          if(!tr.classList.contains(\\\"sub-total\\\")){\\n            subTopTr = tr;\\n            sumTr = subTopTr.cloneNode(true);\\n          }\\n        } \\n        else {\\n          // 합계행이 있으면 \\n          // 현재의 부분합과 동일하면 계산을 누적한다\\n          if(tr.classList.contains(\\\"sub-total\\\")){\\n            // 합계행이 현재행과 다르면 \\n            // 현재행의 이전에 합계행을 추가하고\\n            // 현재행을 합계행으로 지정한다\\n            sumTr.children[rowSumIndex+1].innerText = \\\"합계\\\";\\n            sumTr.classList.add('sub-total');\\n            sumTr.children[rowSumIndex+1].setAttribute('colspan', options.rows.length-(rowSumIndex+1));\\n            for(var i=rowSumIndex+2; i<options.rows.length;i++){\\n              sumTr.children[i].innerText = \\\"\\\";\\n              sumTr.children[i].hidden = true;\\n            }\\n\\n            tr.before(sumTr);\\n            subTopTr = null\\n            sumTr = null\\n          }\\n          else if(sumTr.children[rowSumIndex].innerText == tr.children[rowSumIndex].innerText ){\\n            tr.querySelectorAll('th,td').forEach((el,index)=>{\\n              if (el.tagName==\\\"TD\\\"){\\n                sumTr.children[index].innerText = (parseInt(sumTr.children[index].innerText.replaceAll(\\\",\\\",\\\"\\\")) + parseInt(el.innerText.replaceAll(\\\",\\\",\\\"\\\"))).$toIntFormat();\\n              }\\n            });\\n          } else {\\n            // 합계행이 현재행과 다르면 \\n            // 현재행의 이전에 합계행을 추가하고\\n            // 현재행을 합계행으로 지정한다\\n            sumTr.children[rowSumIndex+1].innerText = \\\"합계\\\";\\n            sumTr.classList.add('sub-total');\\n            sumTr.children[rowSumIndex+1].setAttribute('colspan', options.rows.length-(rowSumIndex+1));\\n\\n            for(var i=rowSumIndex+2; i<options.rows.length;i++){\\n              sumTr.children[i].innerText = \\\"\\\";\\n              sumTr.children[i].hidden = true;\\n            }\\n\\n            tr.before(sumTr);\\n            subTopTr = tr;\\n            sumTr = subTopTr.cloneNode(true);\\n          }\\n        }\\n      })\\n    }\\n  }\\n} \\n\\nfunction totalSum(options){\\n  let lastTr = form.pv.querySelector('tbody tr:last-child');\\n  let sumTr = lastTr.cloneNode(true);\\n  sumTr.classList.add('row-total');\\n  sumTr.querySelectorAll('th,td').forEach((el,index)=>{\\n    if (el.tagName==\\\"TD\\\"){\\n      let sumVal = 0;\\n      form.pv.querySelectorAll('tbody tr:not(.sub-total)').forEach(tr=>{\\n        sumVal +=parseInt(tr.children[index].innerText.replaceAll(\\\",\\\",\\\"\\\")); \\n      })\\n      el.innerText = sumVal.$toIntFormat();\\n    }\\n  })\\n  options.rows.forEach((item, index)=>{\\n    if(index==0){\\n      sumTr.children[index].innerText = \\\"총 합계\\\";\\n      sumTr.children[index].setAttribute('colspan', options.rows.length);\\n    } else {\\n      sumTr.children[index].innerText = \\\"\\\";\\n      sumTr.children[index].hidden = true;\\n    }\\n  })\\n  lastTr.after(sumTr); \\n}\\n\\nfunction rowCate(row, categorys){\\n  let result = [];\\n  categorys.forEach(cat=>{\\n    result.push(row[cat]);\\n  });\\n  return result.join(\\\"|\\\");\\n}\\n\\nfunction jsonPivot(options){\\n  let result = [];\\n\\n  // 열 카다테고리 정리\\n  let colCategories = [];\\n  options.dataSet.forEach(row=>{\\n    if (!colCategories.includes(rowCate(row, options.cols))){\\n      colCategories.push(rowCate(row, options.cols));\\n    }\\n  })\\n\\n  colCategories = colCategories.sort();\\n\\n\\n\\n  options.dataSet.forEach(row => {\\n    //  동일 행조건 파악\\n    let index = result.findIndex((item, index) => {\\n      return rowCate(row, options.rows) == rowCate(item, options.rows);\\n    });\\n\\n    if (index >= 0) {\\n//       if(options.datas.length ==1){\\n//         result[index][rowCate(row, options.cols)] += (row[options.datas[0].field]||0);\\n//       } else {\\n        options.datas.forEach(data=>{\\n          switch(data.math){\\n            case \\\"count\\\":\\n              result[index][rowCate(row, options.cols)+\\\"|\\\"+data.label] += row[data.field]?1:0;  \\n              break;\\n            case \\\"sum\\\":\\n              result[index][rowCate(row, options.cols)+\\\"|\\\"+data.label] += (row[data.field]||0);    \\n              break;\\n          }\\n        })\\n//       }\\n\\n    } else {\\n      let newItem = {};\\n\\n      // 기본행 항목을 추가한다\\n      options.rows.forEach(item => {\\n        newItem[item] = row[item];\\n      });\\n\\n      colCategories.forEach(categorie => {\\n        if (categorie == rowCate(row, options.cols)) {\\n//           if(options.datas.length ==1){\\n//             newItem[categorie] = (newItem[categorie] || 0) + (row[options.datas[0].field]||0);\\n//           } else {\\n            options.datas.forEach(data=>{\\n              switch(data.math){\\n                case \\\"count\\\":\\n                  newItem[categorie+\\\"|\\\"+data.label] = row[data.field]?1:0;  \\n                  break;\\n                case \\\"sum\\\":\\n                  newItem[categorie+\\\"|\\\"+data.label] = (row[data.field]||0);  \\n                  break;\\n              }\\n            })\\n//           }\\n        } else {\\n//           if(options.datas.length ==1){\\n//             newItem[categorie] = 0;\\n//           } else {\\n            options.datas.forEach((data,dfIndex)=>{\\n              switch(data.math){\\n                case \\\"count\\\":\\n                  newItem[categorie+\\\"|\\\"+data.label] = 0;  \\n                  break;\\n                case \\\"sum\\\":\\n                  newItem[categorie+\\\"|\\\"+data.label] = 0;  \\n                  break;\\n              }\\n            })\\n//           }\\n        }\\n      });\\n\\n      result.push(newItem);\\n    }\\n  });\\n\\n  return result;\\n}\\n\\n\\n/**\\n   * @desc JSON 데이터를 HTML 문자열로 변환하여 반환한다.\\n   * @param {Array} - 대상 JSON 데이터\\n   * @return {String} - HTML 문자열\\n   * @example -\\n   */\\nfunction json2html(options) {\\n\\n  let columns = Object.keys(options.pvTable[0]);\\n  let bodyHtml = '<table><colgroup>';\\n\\n  columns.forEach((key, index) => {\\n    let colWidth = 100;\\n    bodyHtml += `<col style=\\\"width: ${colWidth}px;\\\">`;\\n  });\\n\\n  bodyHtml += `</colGroup>`;\\n\\n  bodyHtml += '<thead><tr>';\\n\\n\\n  columns.forEach(key => (bodyHtml += '<th>' + key + '</th>'));\\n\\n  bodyHtml += '</tr></thead><tbody>';\\n\\n  options.pvTable.forEach(row => {\\n    let tr = '<tr>';\\n\\n    options.rows.forEach(item=>{\\n      let value = row[item];\\n      tr += '<th>' + value + '</th>';  \\n    });\\n\\n    columns.forEach(column=>{\\n      if(!options.rows.includes(column)){\\n        let value = row[column];\\n        if (typeof(value)==\\\"number\\\"){\\n          switch(options.numFormat){\\n            case \\\"INT\\\":\\n              tr += '<td>' + value.$toIntFormat() + '</td>';\\n              break;\\n            case \\\"FLOAT\\\":\\n              tr += '<td>' + value.$toFloatFormat() + '</td>';\\n              break;\\n            default:\\n              tr += '<td>' + value + '</td>';\\n              break;\\n          }\\n        } else {\\n          tr += '<td>' + value + '</td>';  \\n        }\\n      }\\n\\n    })\\n\\n    tr += '</tr>';\\n    bodyHtml += tr;\\n  });\\n\\n  bodyHtml += '</tbody></table>';\\n\\n  return bodyHtml;\\n}\\n\\nfunction meargeRow(options) {\\n\\n  options.rows.forEach((row, cellIndex)=>{\\n\\n    let spanTr = null;\\n    let spanTd = null;\\n    let spanCount = 1;\\n    let spanText = null;\\n\\n    form.pv.querySelectorAll(`tr`).forEach(tr => {\\n      if (!spanTr) {\\n        spanTr = tr;\\n        spanTd = spanTr.childNodes[cellIndex];\\n        spanText = spanTd.innerText;\\n        spanCount = 1;\\n      } else {\\n        if (spanText == tr.childNodes[cellIndex].innerText ) {\\n          if(tr.childNodes[cellIndex-1]){\\n            if(tr.childNodes[cellIndex-1].hidden){\\n              spanCount++;\\n              spanTd.setAttribute('rowspan', spanCount);\\n              tr.childNodes[cellIndex].hidden = true;\\n            } else {\\n              spanTr = tr;\\n              spanTd = spanTr.childNodes[cellIndex];\\n              spanText = spanTd.innerText;\\n              spanCount = 1;\\n            }         \\n          }  else {\\n            spanCount++;\\n            spanTd.setAttribute('rowspan', spanCount);\\n            tr.childNodes[cellIndex].hidden = true;\\n          }\\n\\n        } else {\\n          spanTr = tr;\\n          spanTd = spanTr.childNodes[cellIndex];\\n          spanText = spanTd.innerText;\\n          spanCount = 1;\\n        }\\n      }\\n    });\\n  });\\n}\\n\\nfunction meargeCol(options){\\n  /**\\n   * 해드는 컬럼의 갯수로 반복해서 만든다\\n   */\\n  let headTr = form.pv.querySelector('thead tr');\\n  options.cols.forEach((col, index)=>{\\n    if(index>0){\\n      headTr.after(headTr.cloneNode(true));\\n    }                       \\n  });\\n  if(options.datas.length > 1){\\n    headTr.after(headTr.cloneNode(true));\\n  }\\n\\n  /** \\n   * 해드셀 Text작업\\n   */\\n  form.pv.querySelectorAll('thead tr').forEach((tr,index)=>{\\n    tr.querySelectorAll('th').forEach(th=>{\\n      th.innerText = th.innerText.split('|')[index]||th.innerText;\\n    })\\n  });\\n  /** \\n   * 해드셀 머지작업\\n   */  \\n\\n  form.pv.querySelectorAll('thead tr').forEach((tr)=>{\\n    let showTh;\\n    let colSpan = 1;\\n    tr.querySelectorAll('th').forEach(th=>{\\n      if(showTh){\\n        if(showTh.innerText == th.innerText){\\n          th.hidden = true;\\n          colSpan++;\\n          showTh.setAttribute('colspan', colSpan);\\n        } else {\\n          showTh = th;\\n          colSpan = 1;\\n        }\\n      } else {\\n        showTh = th;\\n        colSpan = 0;\\n      }\\n    })\\n  });  \\n}\\n\\n\\n/**\\n   * @desc 지정한 rows 데이터를 지정한 keys의 값을 기준으로 재정렬한다.\\n   * @param {Array} - rows 데이터\\n   * @param {Object} - keys 데이터\\n   * @return {Array} - 변경된 rows 데이터\\n   * @example -\\n   */\\nfunction sortRows(options) {\\n\\n  let keys ={};\\n  let rows = options.pvTable;\\n\\n  options.rows.forEach((row, index)=>keys[row]=\\\"asc\\\")\\n\\n  let obLen = function (obj) {\\n    let size = 0, key;\\n\\n    for (key in obj) {\\n      if (obj.hasOwnProperty(key)) size++;\\n    }\\n\\n    return size;\\n  };\\n\\n  let obIx = function (obj, ix) {\\n    let size = 0, key;\\n\\n    for (key in obj) {\\n      if (obj.hasOwnProperty(key)) {\\n        if (size == ix) return key;\\n\\n        size++;\\n      }\\n    }\\n\\n    return false;\\n  };\\n\\n  let multiSort = function (a, b, d) {\\n    d = d !== null ? d : 1;\\n\\n    if (a == b) return 0;\\n\\n    return a > b ? 1 * d : -1 * d;\\n  };\\n\\n  let KL = obLen(keys);\\n  let newarr = rows.slice(0);\\n\\n  if (!KL) {\\n    return newarr.sort(multiSort);\\n  }\\n\\n  for (let k in keys) {\\n    if (keys[k]) keys[k] = keys[k].toLowerCase();\\n\\n    keys[k] = keys[k] == 'desc' || keys[k] == -1 ? -1 : keys[k] == 'skip' || keys[k] === 0 ? 0 : 1;\\n  }\\n\\n  newarr.sort(function (a, b) {\\n    let sorted = 0, ix = 0;\\n\\n    while (sorted === 0 && ix < KL) {\\n      let k = obIx(keys, ix);\\n\\n      if (k) {\\n        let dir = keys[k];\\n\\n        sorted = multiSort(a[k], b[k], dir);\\n\\n        ix++;\\n      }\\n    }\\n\\n    return sorted;\\n  });\\n\\n  options.pvTable = newarr;\\n  return newarr;\\n}\\n\"\r\n}",
